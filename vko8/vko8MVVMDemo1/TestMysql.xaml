<Window x:Class="MVVMDemo.TestMysql"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:vko8MVVMDemo1"
        mc:Ignorable="d"
        Title="TestMysql" Height="300" Width="500">
    <Grid>
        <DockPanel>
            <Label Content="Insert password:" DockPanel.Dock="Top" />
            <PasswordBox x:Name="pwb" DockPanel.Dock="Top" />
            <Button x:Name="btnGetData" Content="Hae oppilaat MySQL:stä"
                    Click="btnGetData_Click" DockPanel.Dock="Top" />
            <DataGrid x:Name="dgStudents" ItemsSource="{Binding}" />
        </DockPanel>
    </Grid>
</Window>
